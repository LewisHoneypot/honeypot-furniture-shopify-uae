{%- liquid
  capture icon_caret
    render 'icon-caret'
  endcapture
  capture icon_hamburger
    render 'icon-hamburger', style_icon: section.settings.icon_type_style
  endcapture
  capture icon_close
    render 'icon-close', style_icon: section.settings.icon_type_style
  endcapture
  capture icon_search
    render 'icon-search', style_icon: section.settings.icon_type_style
  endcapture
  capture icon_cart
    render 'icon-cart', style_icon: section.settings.icon_type_style
  endcapture
  capture icon_wishlist
    render 'icon-wishlist', style_icon: section.settings.icon_type_style
  endcapture
  capture icon_account
    render 'icon-account', style_icon: section.settings.icon_type_style
  endcapture
  assign has_transparent = false
  if request.page_type == 'index' and section.settings.enable_transparent
    if section.settings.logo_transparent != blank
      assign header_class = 'has-logo-tr transparent pos-absolute top-0 left-0 w-100'
      assign has_transparent = true
      if section.settings.logo == blank
        assign header_class = 'not-logo-header transparent pos-absolute top-0 left-0 w-100'
      endif
    else
      assign header_class = 'transparent pos-absolute top-0 left-0 w-100'
    endif
  endif
  if settings.cart_type == 'drawer'
    render 'cart-drawer'
  endif
  if shop.customer_accounts_enabled
    render 'account-drawer'
  endif
  render 'search-drawer'

  render 'wishlist-drawer'

  assign check_header__icons = false
  if section.settings.enable_country_selector or section.settings.enable_language_selector or section.settings.enable_search or section.settings.enable_search_mobile or section.settings.enable_account or section.settings.enable_wishlist or section.settings.enable_wishlist_mobile or section.settings.enable_cart or section.settings.enable_cart_mobile
    assign check_header__icons = true
  endif

  assign check_header__icons_desktop = false
  unless section.settings.enable_country_selector or section.settings.enable_language_selector or section.settings.enable_search or section.settings.enable_account or section.settings.enable_wishlist or section.settings.enable_cart
    assign check_header__icons_desktop = true  
    assign header__icons_desktop = "disable-icons__desktop"
  endunless

  unless section.settings.enable_search_mobile or section.settings.enable_wishlist_mobile or section.settings.enable_cart_mobile
    assign header__icons_mobile = "disable-icons__mobile"
  endunless
-%}
<{% if section.settings.type_sticky_header != 'none' %}sticky-header{% else %}div{% endif %} class="header-wrapper{% if section.settings.logo != blank or has_transparent %} has--logo{% endif %} header-style-{{ section.settings.type_style }} {{ header_class }} d-block pos-relative{% if section.settings.show_line_separator %} b-bottom{% endif %} header-sticky__{{ section.settings.type_sticky_header }} color-{{ section.settings.color_scheme }} {{ section.settings.text_size | prepend: 'text-size-' }} gradient 
  
  {% if section.settings.use_wide_search == true %}wide-menu-header-wrapper 
    {% if request.page_type != 'index' %} wide-menu-header-wrapper-fix {% endif %}
  {% endif %}
  
  "
  style="
    {%- if section.settings.enable_transparent -%}
      --foreground-transparent: {{ section.settings.text_transparent.red }},{{ section.settings.text_transparent.green }},{{ section.settings.text_transparent.blue }};
      --color_opacity_1: {{ section.settings.color_opacity_1 | times: 0.01 }};
      --color_opacity_2: {{ section.settings.color_opacity_2 | times: 0.01 }};
    {%- endif -%}
    {%- if section.settings.type_style == 'special' and section.settings.logo != blank -%}
      --m-w-logo-special: {{ section.settings.logo_width |  divided_by: 10 }}rem;
    {%- endif -%}
    "
>
  {%- if request.page_type == 'index' and section.settings.logo_position == 'top-center-special' and section.settings.type_style != 'special' -%}
    <div class="header--logo-special m-auto d-block o-h w-100">
      <h1 class="header__heading m-b" style="--padding-top: calc({{ section.settings.padding_top }}px + 1.2rem); --m-b: 0;">
        <a href="{{ routes.root_url }}" class="link link--text u-none break focus-inset d-block o-h center"
          {%- if section.settings.logo != blank -%}
            style="--m-w: {{ section.settings.logo_width }}px;"
          {%- endif -%}
        >
          {%- if section.settings.logo != blank -%}
            {%- assign logo_alt = section.settings.logo.alt | default: shop.name | escape -%}
            {{- section.settings.logo | image_url: width: section.settings.logo.width | image_tag: 
              preload: true, 
              sizes: sizes, 
              widths: '300,400,500,600,800,1000,1200,1400,1600,1800,2000', 
              alt: logo_alt, 
              draggable: false 
            -}}
          {%- else -%}
            <span class="h2 l-h-1 color-light">{{ shop.name }}</span>
          {%- endif -%}
        </a>
      </h1>
    </div>
  {%- endif -%}
  <header 
    class="
      header 
      main--icons-style-{{ section.settings.icon_type_style }}
      {% if section.settings.type_style != 'special' %} 
        header--{{ section.settings.logo_position }}
      {% else %}  
        header--middle-left
      {% endif %}
      {% if section.settings.logo_position == 'middle-left' or section.settings.logo_position == 'middle-right' %}
        {% if section.settings.logo_width >= 110 %} 
          header--nav__center
        {% endif %}
      {% endif %} 
      {{ header__icons_desktop }} 
      {{ header__icons_mobile }}
      {% if section.settings.use_custom_button %} 
        has-button--customized
      {% endif %} 
      d-grid header--mobile-{{ section.settings.mobile_logo_position }} 
      {{ section.settings.section_width }} 
      m-auto
      {% if section.settings.menu != blank %} 
        header--has-menu
      {% endif %} 
      spacing
    " 
    style="{% if section.settings.section_width == 'customized-width' %}--page-width: {{ section.settings.customize_width }}px;{% endif %}
      --padding-top: {{ section.settings.padding_top }}px;
      --padding-bottom: {{ section.settings.padding_bottom }}px;" 
  >
    <!-- start Omniconvert.com code -->
    {% comment %} <link rel="dns-prefetch" href="//app.omniconvert.com" /> {% endcomment %}
    {% comment %} <script type="text/javascript">window._mktz=window._mktz||[];</script> {% endcomment %}
    {% comment %} <script src="//cdn.omniconvert.com/js/xeb7be7.js"></script> {% endcomment %}
    <!-- end Omniconvert.com code -->
    
    {%- if section.settings.menu != blank -%}
      <header-drawer data-breakpoint="md" class="header-drawer icons-style-{{ section.settings.icon_type_style }} d-none-lg{% if section.settings.enable_wishlist_mobile and section.settings.mobile_logo_position == 'center' %} a-i-center{% endif %}">
        {%- if section.settings.mobile_logo_position == 'left' -%}
          {%- if section.settings.enable_search or section.settings.enable_search_mobile -%}
            <side-drawer-opener class="header__icon header__icon--search d-flex j-c-center a-i-center p-0 u-none color-foreground f-a-none{% unless section.settings.enable_search %} d-none-l{% endunless %}{% unless section.settings.enable_search_mobile %} d-none-mw-l{% endunless %}" data-side-drawer="#Drawer-Search">
              <button class="t-button b-none c-pointer p-0 color-foreground bg-transparent focus-inset no-js-hidden {{ section.settings.font_transform }}" type="button" aria-haspopup="dialog" aria-label="{{ 'general.search.search' | t }}">{{ icon_search }}</button>
              <noscript>
                <a href="{{ routes.search_url }}" class="link u-none color-foreground focus-inset {{ section.settings.font_transform }}">
                  {{ icon_search }}
                  <span class="visually-hidden">{{ 'general.search.search' | t }}</span>
                </a>
              </noscript>
            </side-drawer-opener>
          {%- endif -%}
          {%- if section.settings.enable_account or section.settings.enable_account_mobile -%}
            {%- if shop.customer_accounts_enabled -%}
              <side-drawer-opener class="header__icon header__icon--account d-flex j-c-center a-i-center p-0 u-none color-foreground f-a-none{% unless section.settings.enable_account %} d-none-l{% endunless %}{% unless section.settings.enable_account_mobile %} d-none-mw-md{% endunless %}" data-side-drawer="#Drawer-Account">
                <button class="t-button b-none c-pointer p-0 color-foreground bg-transparent focus-inset no-js-hidden {{ section.settings.font_transform }}" type="button" aria-haspopup="dialog" aria-label="{% if customer %}{{ 'customer.account_fallback' | t }}{% else %}{% if section.settings.icon_type_style == 'default-text' %}{{ 'customer.account.title' | t }}{% else %}{{ 'customer.log_in' | t }}{% endif %}{% endif %}">
                  {{ icon_account }}
                </button>
                <noscript>
                  <a href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}" class="link u-none color-foreground focus-inset {{ section.settings.font_transform }}">
                    {{ icon_account }}
                    <span class="visually-hidden">
                      {%- liquid
                        if customer
                          echo 'customer.account_fallback' | t
                        else
                          if section.settings.icon_type_style == 'default-text'
                            echo 'customer.account.title' | t
                          else
                            echo 'customer.log_in' | t
                          endif
                        endif
                      -%}
                    </span>
                  </a>
                </noscript>
              </side-drawer-opener>
            {%- endif -%}
          {%- endif -%}
          {%- if section.settings.enable_wishlist or section.settings.enable_wishlist_mobile -%}
            <side-drawer-opener class="header__icon header__icon--wishlist d-flex j-c-center a-i-center p-0 pos-relative u-none color-foreground f-a-none{% unless section.settings.enable_wishlist %} d-none-md{% endunless %}{% unless section.settings.enable_wishlist_mobile %} d-none-xs{% endunless %}" data-side-drawer="#Drawer-Wishlist">
              <button class="t-button b-none c-pointer p-0 color-foreground bg-transparent focus-inset no-js-hidden {{ section.settings.font_transform }}{% if section.settings.icon_type_style == 'default-text' %} d-flex c-gap{% endif %}" 
                type="button" 
                aria-haspopup="dialog" 
                aria-label="{{ 'templates.wishlist.wishlist_items' | t: count: cart.item_count }}"
                {%- if section.settings.icon_type_style == 'default-text' %}
                  style="--c-gap: 0.3rem"
                {% endif -%}
              >
                {{ icon_wishlist }}
                {%- if section.settings.icon_type_style != 'default-text' -%}
                  <div class="wishlist-count-bubble l-h-1 pos-absolute b-circle d-flex j-c-center a-i-center">
                    <span class="number" aria-hidden="true">0</span>
                    <span class="visually-hidden" data-text="{{ 'templates.wishlist.items' | t }}">0 {{ 'templates.wishlist.items' | t }}</span>
                  </div>
                {%- else -%}
                  <div class="wishlist-text-count wishlist-count-bubble d-flex function-menu-item">  
                    (<div class="wishlist-icon-bubble">
                      <span class="number" aria-hidden="true">0</span>
                      <span class="visually-hidden" data-text="{{ 'templates.wishlist.items' | t }}">0 {{ 'templates.wishlist.items' | t }}</span>
                    </div>)
                  </div>
                {%- endif -%}
              </button>
              <noscript>
                <a href="{{ routes.cart_url }}" class="link u-none color-foreground focus-inset {{ section.settings.font_transform }}">
                  {{ icon_wishlist }}
                  <span class="visually-hidden">{{ 'templates.wishlist.wishlist' | t }}</span>
                </a>
              </noscript>
            </side-drawer-opener>
          {%- endif -%}
          {%- if section.settings.enable_cart or section.settings.enable_cart_mobile -%}
            {%- if settings.cart_type == 'drawer' -%}
              <side-drawer-opener class="header__icon header__icon--cart d-flex j-c-center a-i-center p-0 pos-relative u-none color-foreground f-a-none{% unless section.settings.enable_cart %} d-none-l{% endunless %}{% unless section.settings.enable_cart_mobile %} d-none-mw-l{% endunless %}" data-side-drawer="#Cart-Drawer">
                <button class="t-button b-none c-pointer p-0 color-foreground bg-transparent focus-inset no-js-hidden {{ section.settings.font_transform }}{% if section.settings.icon_type_style == 'default-text' %} d-flex c-gap{% endif %}" 
                  type="button" 
                  aria-haspopup="dialog" 
                  aria-label="{{ 'templates.cart.cart_items' | t: count: cart.item_count }}"
                  {%- if section.settings.icon_type_style == 'default-text' %}
                    style="--c-gap: 0.3rem"
                  {% endif -%}
                >
                  {{ icon_cart }}
                  {%- if cart.item_count < 100 -%}
                    {%- if section.settings.icon_type_style != 'default-text' -%}
                      <div class="cart-count-bubble l-h-1 pos-absolute b-circle d-flex j-c-center a-i-center cart-icon-bubble">
                        <span aria-hidden="true">{{ cart.item_count }}</span>
                        <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                      </div>
                    {%- else -%}
                      <div class="cart-text-count d-flex function-menu-item">  
                        (<div class="cart-icon-bubble">
                          <span aria-hidden="true">{{ cart.item_count }}</span>
                          <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                        </div>)
                      </div>
                    {%- endif -%}
                  {%- endif -%}
                </button>
                <noscript>
                  <a href="{{ routes.cart_url }}" class="link u-none color-foreground focus-inset {{ section.settings.font_transform }}{% if section.settings.icon_type_style == 'default-text' %} d-flex c-gap{% endif %}"
                    {%- if section.settings.icon_type_style == 'default-text' %}
                      style="--c-gap: 0.3rem"
                    {% endif -%}
                  >
                    {{ icon_cart }}
                    <span class="visually-hidden">{{ 'templates.cart.cart' | t }}</span>
                    {%- if cart.item_count < 100 -%}
                      {%- if section.settings.icon_type_style != 'default-text' -%}
                        <div class="cart-count-bubble l-h-1 pos-absolute b-circle d-flex j-c-center a-i-center cart-icon-bubble">
                          <span aria-hidden="true">{{ cart.item_count }}</span>
                          <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                        </div>
                      {%- else -%}
                        <div class="cart-text-count d-flex function-menu-item">  
                          (<div class="cart-icon-bubble">
                            <span aria-hidden="true">{{ cart.item_count }}</span>
                            <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                          </div>)
                        </div>
                      {%- endif -%}
                    {%- endif -%}
                  </a>
                </noscript>
              </side-drawer-opener>
            {%- else -%}
              <a href="{{ routes.cart_url }}" class="header__icon header__icon--cart d-flex j-c-center a-i-center p-0 u-none color-foreground pos-relative f-a-none focus-inset{% if section.settings.icon_type_style == 'default-text' %} d-flex c-gap{% endif %}{% unless section.settings.enable_cart %} d-none-l{% endunless %}{% unless section.settings.enable_cart_mobile %} d-none-mw-l{% endunless %}"
                {%- if section.settings.icon_type_style == 'default-text' %}
                  style="--c-gap: 0.3rem"
                {% endif -%}
              >
                {{ icon_cart }}
                <span class="visually-hidden">{{ 'templates.cart.cart' | t }}</span>
                {%- if cart.item_count < 100 -%}
                  {%- if section.settings.icon_type_style != 'default-text' -%}
                    <div class="cart-count-bubble l-h-1 pos-absolute b-circle d-flex j-c-center a-i-center cart-icon-bubble">
                      <span aria-hidden="true">{{ cart.item_count }}</span>
                      <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                    </div>
                  {%- else -%}
                    <div class="cart-text-count d-flex function-menu-item">  
                      (<div class="cart-icon-bubble">
                        <span aria-hidden="true">{{ cart.item_count }}</span>
                        <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                      </div>)
                    </div>
                  {%- endif -%}
                {%- endif -%}
              </a>
            {%- endif -%}
          {%- endif -%}
        {%- endif -%}
        <details id="Details-menu-drawer-container" class="menu-drawer-container d-flex">
          <summary class="header__icon header__icon--menu header__icon--summary link u-none color-foreground p-0 c-pointer pos-relative f-a-none focus-inset" aria-label="{{ 'sections.header.menu' | t }}">
            <span class="d-flex j-c-center a-i-center h-100 {{ section.settings.font_transform }}">
              {{ icon_hamburger }}
              {{ icon_close }}
            </span>
          </summary>
          <div id="menu-drawer" class="menu-drawer motion-reduce pos-absolute top-100 left-0 z-index-3 p-0 gradient gradient-global" tabindex="-1">
            <div class="menu-drawer__inner-container pos-relative h-100">
              <div class="menu-drawer__navigation-container d-grid h-100">
                <nav class="menu-drawer__navigation">
                  <ul class="menu-drawer__menu has-submenu list-menu list-unstyled" role="list">
                    {%- for link in section.settings.menu.links -%}
                      <li class="b-bottom">
                        {%- if link.links != blank -%}
                          <details id="Details-menu-drawer-menu-item-{{ forloop.index }}">
                            <summary class="menu-drawer__menu-item link u-none color-foreground focus-inset d-flex c-pointer pos-relative">
                              {{ link.title | escape }}
                              {% render 'icon-arrow' %}
                              {{ icon_caret }}
                            </summary>
                            <div id="link-{{ link.handle | escape }}" class="menu-drawer__submenu menu-drawer__vl-2 has-submenu gradient motion-reduce" tabindex="-1">
                              <div class="menu-drawer__inner-submenu h-100">
                                <button class="menu-drawer__close-button link focus-inset d-flex a-i-center t-button b-none c-pointer w-100 left f-bold" aria-expanded="true">
                                  {% render 'icon-arrow' %}
                                  {{ link.title | escape }}
                                </button>
                                <ul class="menu-drawer__menu list-menu list-unstyled" role="list" tabindex="-1">
                                  <li class="b-bottom">
                                    <a href="{{ link.url }}" class="menu-drawer__menu-item link u-none color-foreground focus-inset d-flex"{% if childlink.current %} aria-current="page"{% endif %}>
                                        {{ 'sections.header.go_to' | t: name: link.title | escape }}
                                    </a>
                                  </li>
                                  {%- for childlink in link.links -%}
                                    <li class="b-bottom">
                                      {%- if childlink.links == blank -%}
                                        <a href="{{ childlink.url }}" class="menu-drawer__menu-item link u-none color-foreground focus-inset d-flex"{% if childlink.current %} aria-current="page"{% endif %}>
                                          {{ childlink.title | escape }}
                                        </a>
                                      {%- else -%}
                                        <details id="Details-menu-drawer-submenu-{{ forloop.index }}">
                                          <summary class="menu-drawer__menu-item link u-none color-foreground focus-inset d-flex c-pointer pos-relative">
                                            {{ childlink.title | escape }}
                                            {% render 'icon-arrow' %}
                                            {{ icon_caret }}
                                          </summary>
                                          <div id="childlink-{{ childlink.handle | escape }}" class="menu-drawer__submenu menu-drawer__vl-3 has-submenu gradient motion-reduce">
                                            <button class="menu-drawer__close-button link u-none focus-inset d-flex a-i-center t-button b-none c-pointer w-100 left f-bold" aria-expanded="true">
                                              {% render 'icon-arrow' %}
                                              {{ childlink.title | escape }}
                                            </button>
                                            <ul class="menu-drawer__menu list-menu list-unstyled" role="list" tabindex="-1">
                                              <li class="b-bottom">
                                                <a href="{{ childlink.url }}" class="menu-drawer__menu-item link u-none color-foreground focus-inset d-flex"{% if grandchildlink.current %} aria-current="page"{% endif %}>
                                                    {{ 'sections.header.go_to' | t: name: childlink.title | escape }}
                                                </a>
                                              </li>
                                              {%- for grandchildlink in childlink.links -%}
                                                <li class="b-bottom">
                                                  <a href="{{ grandchildlink.url }}" class="menu-drawer__menu-item link u-none color-foreground focus-inset d-flex"{% if grandchildlink.current %} aria-current="page"{% endif %}>
                                                    {{ grandchildlink.title | escape }}
                                                  </a>
                                                </li>
                                              {%- endfor -%}
                                            </ul>

                                            {% comment %} Mega drawer banner {% endcomment %}
                                            {%- liquid
                                              assign megamenu_1 = section.blocks | where: "type", "mega_menu_1"
                                              assign megamenu_2 = section.blocks | where: "type", "mega_menu_2"
                                              assign check_megamenu = false
                                              if megamenu_1.size > 0 or megamenu_2.size > 0
                                                  assign check_megamenu = true
                                              endif
                                              assign has_megamenu = false
                                              assign menu_title = link.title | escape | downcase | handle
                                              for block in section.blocks
                                                assign block_title = block.settings.menu_item | escape | downcase | handle
                                                if block_title == menu_title
                                                  assign has_megamenu = true
                                                  case block.type
                                                    when 'mega_menu_1'
                                                      assign menu_type = 'mega_menu_1'
                                                      assign section_width = block.settings.section_width
                                                      assign customize_width = block.settings.customize_width
                                                      assign mg1_image_1 = block.settings.image_1
                                                      assign mg1_text_image_1 = block.settings.text_image_1
                                                      assign mg1_link_image_1 = block.settings.link_image_1
                                                      assign image_overlay_opacity = block.settings.image_overlay_opacity
                                                      assign color_overlay_opacity = block.settings.color_overlay_opacity
                                                    when 'mega_menu_2'
                                                      assign menu_type = 'mega_menu_2'
                                                      assign section_width = block.settings.section_width
                                                      assign customize_width = block.settings.customize_width
                                                      assign mg2_image_1 = block.settings.image_1
                                                      assign mg2_image_2 = block.settings.image_2
                                                      assign mg2_image_3 = block.settings.image_3
                                                      assign mg2_image_4 = block.settings.image_4
                                                      assign mg2_text_image_1 = block.settings.text_image_1
                                                      assign mg2_text_image_2 = block.settings.text_image_2
                                                      assign mg2_text_image_3 = block.settings.text_image_3
                                                      assign mg2_text_image_4 = block.settings.text_image_4
                                                      assign mg2_link_image_1 = block.settings.link_image_1
                                                      assign mg2_link_image_2 = block.settings.link_image_2
                                                      assign mg2_link_image_3 = block.settings.link_image_3
                                                      assign mg2_link_image_4 = block.settings.link_image_4
                                                      assign heading_font_size = block.settings.heading_font_size
                                                  endcase
                                                endif
                                              endfor
                                            -%}
                                            {%- if check_megamenu and has_megamenu -%}
                                              {%- if menu_type == 'mega_menu_1' -%}
                                                {%- if mg1_image_1 != blank -%}
                                                  <div class="mega-drawer__banner mega-1__drawer d-flex grid o-h {{ link.title }} {{ block_title }}" style="--c-xs: 1; --c-sm: 1; --c-md: 1; --c-lg: 1;">
                                                    <{% if mg1_link_image_1 != blank %}a{% else %}div{% endif %} class="mega-menu__item grid__item pos-relative"{% if mg1_link_image_1 != blank %} href="{{ mg1_link_image_1 }}" aria-label="{{ mg1_image_1.alt | escape }}"{% endif %}
                                                      style="
                                                        --media-opacity: {{ image_overlay_opacity | divided_by: 100.0 }};
                                                        --color-opacity: {{ color_overlay_opacity.red }},{{ color_overlay_opacity.green }},{{ color_overlay_opacity.blue }};
                                                      "
                                                    >
                                                        <div class="media media--transparent media--adapt media--zoom d-block pos-relative z-index--1 o-h" style="padding-bottom: {{ 1 | divided_by: mg1_image_1.aspect_ratio | times: 100 }}%;">
                                                          <img
                                                            srcset="{%- if mg1_image_1.width >= 210 -%}{{ mg1_image_1 | image_url: width: 210 }} 210w,{%- endif -%}
                                                                    {%- if mg1_image_1.width >= 390 -%}{{ mg1_image_1 | image_url: width: 390 }} 390w,{%- endif -%}
                                                                    {%- if mg1_image_1.width >= 570 -%}{{ mg1_image_1 | image_url: width: 570 }} 570w,{%- endif -%}
                                                                    {%- if mg1_image_1.width >= 710 -%}{{ mg1_image_1 | image_url: width: 710 }} 710w,{%- endif -%}
