    "image_with_text_zJhwhJ": {
      "type": "image-with-text",
      "blocks": {
        "heading_FtzRWC": {
          "type": "heading",
          "settings": {
            "heading": "<h3>Live Delivery Tracking<\/h3>",
            "heading_size": "h1",
            "padding_top": 10,
            "padding_bottom": 10
          }
        },
        "text_hB8Ee3": {
          "type": "text",
          "settings": {
            "text": "<p>Tired of being given a 12-hour time window for deliveries? We've got you covered!<br\/><br\/>On the day of your delivery, you'll receieve a live tracking link with regular, real-time updates on your expected delivery time. <br\/><br\/>Whether you want to pop home from work quickly to take the delivery or want to enjoy your day off, you'll be able to see our drivers whereabouts in real time.<\/p>",
            "padding_top": 10,
            "padding_bottom": 10
          }
        }
      },
